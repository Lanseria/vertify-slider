{
  "name": "@lanseria/vertify-slider",
  "description": "vertify-slider",
  "version": "1.0.0",
  "files": [
    "dist",
    "dist/*.css"
  ],
  "main": "./dist/vertify-slider.umd.js",
  "module": "./dist/vertify-slider.es.js",
  "exports": {
    ".": {
      "import": "./dist/vertify-slider.es.js",
      "require": "./dist/vertify-slider.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lanseria/vertify-slider.git"
  },
  "homepage": "https://github.com/Lanseria/vertify-slider#readme",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "upi": "yarn upgrade-interactive",
    "release:package": "npm run build && npm publish"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.10.0",
    "@vueuse/core": "^5.0.3",
    "naive-ui": "^2.15.1",
    "pdf-lib": "^1.16.0",
    "pdfjs-dist": "^2.8.335",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "less": "^4.1.1",
    "typescript": "^4.3.2",
    "vite": "^2.3.8",
    "vue-tsc": "^0.0.24"
  }
}
